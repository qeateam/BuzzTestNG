			<objectProperty>xpath=/html/body/app-root/app-page-not-found/h1</objectProperty>
			<text1>Oops! Page not found !</text1>
		</object>
		<object objectId="verifyNotFoundErrorOnPetTypesScreen">
			<objectProperty>xpath=/html/body/app-root/app-page-not-found/div/div/h2</objectProperty>
			<text1>Not Found - 404 error</text1>
		</object>
	</screen>
