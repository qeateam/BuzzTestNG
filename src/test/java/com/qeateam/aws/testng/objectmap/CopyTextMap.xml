	<!-- Start - Objects for Home Page -->
	<screen screenId="petclinicHome">
		<object objectId="welcomeMsgObj">
			<objectProperty>xpath=html/body/app-root/app-welcome/h1</objectProperty>
			<text1>Welcome to Dr. Dolittle Petclinic</text1>
		</object>
		<object objectId="homeLinkTextObj">
			<objectProperty>xpath=html/body/app-root/div/nav/div/ul/li[1]/a/span[2]</objectProperty>
