		<object objectId="searchAllObj">
			<objectProperty>xpath=/html/body/app-root/div/nav/div/ul/li[3]/ul/li[1]/a</objectProperty>
			<text1>All</text1>
		</object>
		<object objectId="addNewObj">
			<objectProperty>xpath=/html/body/app-root/div/nav/div/ul/li[3]/ul/li[2]/a</objectProperty>
			<text1>Add New</text1>
		</object>
		<object objectId="veterinariansHeading">
			<objectProperty>xpath=/html/body/app-root/app-vet-list/div/div/h2</objectProperty>
			<text1>Veterinarians</text1>
		</object>
		<object objectId="veterinariansTableContentsName">
			<objectProperty>xpath=//*[@id="vets"]/thead/tr/th[1]</objectProperty>
			<text1>Name</text1>
		</object>
		<object objectId="veterinariansTableContentsSpecialties">
			<objectProperty>xpath=//*[@id="vets"]/thead/tr/th[2]</objectProperty>
			<text1>Specialties</text1>
		</object>
		<object objectId="veterinariansHomeButton">
			<objectProperty>xpath=/html/body/app-root/app-vet-list/div/div/div/button[1]</objectProperty>
			<text1>Home</text1>
		</object>
		<object objectId="veterinariansAddVetButton">
			<objectProperty>xpath=/html/body/app-root/app-vet-list/div/div/div/button[2]</objectProperty>
			<text1>Add Vet</text1>
		</object>
		<object objectId="verifyHomeScreenOnClickingHomeButton">
			<objectProperty>xpath=/html/body/app-root/app-welcome/h1</objectProperty>
			<text1>Welcome to Dr. Dolittle Petclinic</text1>
		</object>
		<object objectId="verifyAddVetScreen">
			<objectProperty>xpath=/html/body/app-root/app-vet-add/p</objectProperty>
			<text1>vet-add works!</text1>
		</object>
	</screen>
	<!-- End - Objects for Veterinarians Page -->


	<!-- Start - Objects for Pet Types Page -->
	<screen screenId="petClinicPetTypesPage">
